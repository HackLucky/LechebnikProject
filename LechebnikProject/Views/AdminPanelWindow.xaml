<Window x:Class="LechebnikProject.Views.AdminPanelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ЛЕЧЕБНИК [окно панели администратора]" Height="540" Width="960" WindowStartupLocation="CenterScreen">
    <StackPanel Margin="10">
        <Button Content="Открыть управление пользователями" Command="{Binding ManageUsersCommand}" Margin="0,0,0,10"/>
        <Button Content="Открыть управление препаратами" Command="{Binding ManageMedicinesCommand}" Margin="0,0,0,10"/>
        <Button Content="Открыть управление отчётами" Command="{Binding ManageReportsCommand}" Margin="0,0,0,10"/>
        <Button Content="Вернуться к главному меню" Command="{Binding GoBackCommand}"/>
    </StackPanel>
</Window>