<Window x:Class="LechebnikProject.Views.AdminPanelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Панель администратора" Height="300" Width="400">
    <StackPanel Margin="10">
        <Button Content="Управление пользователями" Command="{Binding ManageUsersCommand}" Margin="0,0,0,10"/>
        <Button Content="Управление препаратами" Command="{Binding ManageMedicinesCommand}" Margin="0,0,0,10"/>
        <Button Content="Просмотр отчётов" Command="{Binding ViewReportsCommand}" Margin="0,0,0,10"/>
        <Button Content="Назад" Command="{Binding GoBackCommand}"/>
    </StackPanel>
</Window>